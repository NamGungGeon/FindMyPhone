<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/keyManageWrapper"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:background="#80c89f"
        android:layout_height="match_parent">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="65dp"
            android:background="#59C291"
            android:layout_marginBottom="20dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:textSize="25dp"
                android:textColor="#FFFFFF"
                android:text="키 관리"/>
        </RelativeLayout>
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_centerHorizontal="true"
                    android:layout_alignParentTop="true"
                    android:id="@+id/keyManageWrapper1">
                    <Button
                        android:layout_width="150dp"
                        android:layout_height="150dp"
                        android:layout_margin="15dp"
                        android:id="@+id/changeKey"
                        android:background="@drawable/key_change"/>
                    <Button
                        android:layout_width="150dp"
                        android:layout_height="150dp"
                        android:layout_margin="15dp"
                        android:id="@+id/deleteKey"
                        android:background="@drawable/key_delete"/>
                </LinearLayout>
            </RelativeLayout>
        </ScrollView>
    </LinearLayout>


</FrameLayout>